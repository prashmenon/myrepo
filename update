sudo apt update && sudo apt upgrade -y
sudo apt install guake cowsay fortune-mod sl cmatrix libreoffice -y

###Clean up####
sudo apt autoremove -y
sudo apt autoclean -y
sudo apt dist-upgrade -y
sudo apt full-upgrade -y
####Set background image####
gsettings set org.gnome.desktop.background picture-uri http://prashmenon.com:8080/image/tux.jpg
